                    <CommandText>SELECT
	ISNULL(patient.txtPatientName, '') + ' ' + ISNULL(patient.txtPatientSecondName, '') + ' ' + ISNULL(patient.txtPatientSurname, '') as Фио,
	patient.datBirthday as "Дата рождения",
